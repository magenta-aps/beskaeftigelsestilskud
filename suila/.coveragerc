[run]
relative_files = True
branch = True
source = .
omit =
    */manage.py
    */project/*
    */apps.py
    */urls.py
    */tests.py
    */__init__.py
    */migrations/*
    */tests/*
    */mitid_test/*
    */suila/simulation.py
    */suila/integrations/eskat/responses/*
    */data_update/*
    */management/commands/make_messages.py
    */management/commands/create_user.py
    */management/commands/export_users.py
    */management/commands/import_users.py
    */management/commands/wait_for_db.py
parallel = true
concurrency = multiprocessing
